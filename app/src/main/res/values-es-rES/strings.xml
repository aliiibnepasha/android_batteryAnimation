    <string name="previous">Anterior</string>
    <string name="how_to_use">Cómo usar</string>
    <string name="got_it">¡Entendido!</string>
    <string name="text_accessibility_1">1. Ve a “Apps instaladas” o “Apps descargadas” en ajustes de accesibilidad</string>
